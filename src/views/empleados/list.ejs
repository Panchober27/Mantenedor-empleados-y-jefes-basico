<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <%- include('../layouts/_head.ejs') %>
        <title>Lista empleados.</title>
</head>
<body>
    <%- include('../layouts/_nav.ejs') %>
        <h1 style="text-align: center;">
            Listado de empleadosüëç
        </h1>
        <div class="container">
            <div class="row">
                <% for(let i=0; i < emps.length; i++) { %>
                    <div class="col-12 col-md-6">
                        <div class="card">
                            <div class="card-body">
                                <h3 class="card-title">
                                    <%= emps[i].nombre %> <%= emps[i].apellido_pat %>
                                </h3>
                                <p class="card-text">
                                    <%= emps[i].rut %>
                                </p>
                            </div>
                        </div>
                    </div>
                <% } %>
            </div>
        </div>
        
        <%- include('../layouts/_footer.ejs') %>
</body>
</html>